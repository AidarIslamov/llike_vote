{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch index.ts",
    "docker:start": "npm run db:setup && npm run dev",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:seed:undo && npm run db:seed",
    "db:fresh": "npm run db:migrate:reset && npm run db:seed",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/forwarded": "^3.0.1",
    "fastify": "^5.6.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@types/sequelize": "^4.28.20",
    "@types/validator": "^13.15.3",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
